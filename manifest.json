{
  "manifest_version": 2,
  "name": "Google Drive Quick Search",
  "description" : "Lets you quickly search and get results using the address bar",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "version": "0.2",
  "background": {
  "page": "background.html"
  },
  "omnibox": { "keyword" : "drive" },
  "permissions": [

    "storage",
    "tabs",
    "identity",

    "https://ssl.gstatic.com/",
    "https://www.googleapis.com/",
    "https://accounts.google.com/"
  ],
  "oauth2": {
    "client_id": "485278876310-125309idkfj37tnebu8r4cuv8kdnfm1p.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive"
    ]
  }
}
